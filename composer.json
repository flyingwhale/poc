        }
    ],
   "require": {
        "php":                      ">=5.3.3",
        "monolog/monolog":          "1.*",
        "symfony/event-dispatcher": ">=2.1",
        "flyingwhale/optionable":   "dev-master",
        "flyingwhale/poc-cache":    "dev-master",
        "symfony/console":          "2.*",
        "flyingwhale/plugin":       "0.1"
    },
   "require-dev": {
        "symfony/class-loader":     "2.*",
        "phpunit/phpunit":          "3.7.*",
        "phpunit/dbunit":           "1.2.*",
        "fabpot/php-cs-fixer":      "dev-master",
        "satooshi/php-coveralls":   "dev-master"
    },
   "autoload": {
        "psr-0": { "Poc": "src/" }
